#include <stdio.h>

int main(int argc, const char * argv[]) {
    int a,b,c,d,e;
    system("clear");
basla:
    e++;
    if(e==10){e=0;d++;}
    if(d==10){d=0;c++;}
    if(c==10){c=0;b++;}
    if(b==10){b=0;a++;}
    printf("\n%d",a*10000+b*1000+c*100+d*10+e);
    getchar();
    goto basla;
}
